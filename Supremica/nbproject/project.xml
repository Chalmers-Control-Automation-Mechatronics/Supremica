<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>Supremica</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>Supremica</name>
            <properties>
                <property name="project.dir">trunk</property>
                <property name="ant.script">${project.dir}/build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>trunk</label>
                    <location>${project.dir}</location>
                </source-folder>
                <source-folder>
                    <label>Supremica</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>${project.dir}/src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>tests\src</label>
                    <type>java</type>
                    <location>${project.dir}/tests/src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>compile_supremica</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>doc</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>runLocal</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>compile_supremica</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>${project.dir}/jar</location>
                <script>${ant.script}</script>
                <build-target>initialize</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>${project.dir}/src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>tests\src</label>
                        <location>${project.dir}/tests/src</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>${project.dir}/src</package-root>
                <classpath mode="compile">${project.dir}/lib/unjared</classpath>
                <built-to>${project.dir}/jar</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>${project.dir}/tests/src</package-root>
                <unit-tests/>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-editor-indent">
                <node name="CodeStyle">
                    <property name="usedProfile" value="default"/>
                    <node name="project">
                        <property name="tab-size" value="8"/>
                        <property name="text-limit-width" value="80"/>
                    </node>
                </node>
                <node name="text">
                    <node name="x-java">
                        <node name="CodeStyle">
                            <node name="project"/>
                        </node>
                    </node>
                </node>
            </module>
        </preferences>
    </configuration>
</project>
