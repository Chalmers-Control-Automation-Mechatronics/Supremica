@des debouncing {
  @stateNames {
    FLIP
    INIT
    CHG
    WAIT
  }
  @markedStates {
    INIT
  }
  @initial {
    INIT
  }
  @controllable {
    chg_value.I
    reset.I
    timer.I
  }
  @uncontrollable {
    new_value.I
    timeout.I
  }
  @transitions {
    [new_value.I] FLIP -> INIT
    [timer.I] FLIP -> WAIT
    [reset.I] FLIP -> FLIP
    [timeout.I] FLIP -> FLIP
    [timeout.I] INIT -> INIT
    [new_value.I] INIT -> FLIP
    [reset.I] INIT -> INIT
    [new_value.I] CHG -> INIT
    [chg_value.I] CHG -> INIT
    [new_value.I] WAIT -> INIT
    [timeout.I] WAIT -> CHG
  }
}

