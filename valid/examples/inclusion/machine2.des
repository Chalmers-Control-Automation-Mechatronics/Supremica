@des machine2 {
  @stateNames {
    Working2
    Idle2
    Down2
  }
  @markedStates {
    Idle2
  }
  @initial {
    Idle2
  }
  @controllable {
    Start2
    Repair2
  }
  @uncontrollable {
    Finish2
    Break2
  }
  @transitions {
    [Break2] Working2 -> Down2
    [Finish2] Working2 -> Idle2
    [Start2] Idle2 -> Working2
    [Repair2] Down2 -> Idle2
  }
}

