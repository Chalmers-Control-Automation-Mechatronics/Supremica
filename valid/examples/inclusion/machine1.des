@des machine1 {
  @stateNames {
    Working1
    Idle1
    Down1
  }
  @markedStates {
    Idle1
  }
  @initial {
    Idle1
  }
  @controllable {
    Start1
    Repair1
  }
  @uncontrollable {
    Finish1
    Break1
  }
  @transitions {
    [Break1] Working1 -> Down1
    [Finish1] Working1 -> Idle1
    [Start1] Idle1 -> Working1
    [Repair1] Down1 -> Idle1
  }
}

