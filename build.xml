<target name="initialize" depends="clean">
 	<mkdir dir="${build}"/>
	<delete includeEmptyDirs="true" >
		<fileset dir="${build}" />
	</delete>

 	<mkdir dir="${testsbuild}"/>
	<delete includeEmptyDirs="true" >
		<fileset dir="${testsbuild}" />
	</delete>

