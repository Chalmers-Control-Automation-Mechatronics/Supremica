<target name="run_tests_supremica" depends="compile_supremica,compile_tests_supremica">
	<java fork="yes" classname="junit.textui.TestRunner"
		taskname="junit" failonerror="true">
		<arg value="org.supremica.TestPackageSupremica"/>
		<classpath>	
			<pathelement location="${build}" />
			<pathelement location="${testsbuild}" />
			<pathelement path="${lib}/${unjared}" />
			<pathelement path="${java.class.path}" />
		</classpath>	
	</java>
</target>

<target name="doc_supremica">
	<javadoc
		sourcepath="${src}"
		destdir="${doc}/api"
		packagenames="${supremica_basepackage}.automata.*,${supremica_basepackage}.gui.*,${supremica_basepackage}.util.*,${supremica_basepackage}.apps.*"
		classpath="${lib}/${unjared}"
		author="true"
		version="true"
		use="true"
		private="true"
		windowtitle="Supremica API"
		overview="${templates}/Overview.html"
		doctitle="Supremica"
		
		bottom="Copyright &#169; 2001, 2002, 2003 Supremica and Knut Akesson. All Rights Reserved.">
	</javadoc>
</target>

