<!--
*	If you for some reason prefer javac instead of jikes,
*	just change "jikes" to "classic" on the next line.
-->
<property name="build.compiler" value="jikes"/>
<!-- Enables emacs warning warning and error mesages. This is equals to calling jikes +E -->
<!-- Put this in TextPad to get clickable error messages
	Expression = ^[ \t]+\[javac\] \([A-Z]:[^:]+\):\([0-9]+\):\([0-9]+\):\([0-9]+\):
	File Register = 1
	Line Register = 2
	Column Register = 3
-->
<property name="build.compiler.emacs" value="true"/>
<!--
<property name="build.compiler.emacs" value="true"/>
<property name="build.compiler.pedantic" value="false"/>
-->

