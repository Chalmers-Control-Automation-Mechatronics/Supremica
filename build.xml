	<delete includeEmptyDirs="true" >
		<fileset dir="${dist}" />
	</delete>

	<mkdir dir="${lib}/${unjared}"/>
	<delete includeEmptyDirs="true" >
		<fileset dir="${lib}/${unjared}" />
	</delete>

	<mkdir dir="${resources}"/>
	<delete includeEmptyDirs="true" >
		<fileset dir="${resources}" />
	</delete>
 
	<unjar src="${lib}/JGo.jar" dest="${lib}/${unjared}"/>	
	<unjar src="${lib}/JGoLayout.jar" dest="${lib}/${unjared}"/>
