	<unjar src="${lib}/junit.jar" dest="${lib}/${unjared}"/>	
	<unjar src="${lib}/log4j-core.jar" dest="${lib}/${unjared}"/>
	<unjar src="${lib}/log4j.jar" dest="${lib}/${unjared}"/>
