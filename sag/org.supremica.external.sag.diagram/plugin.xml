   <extension point="org.eclipse.ui.navigator.navigatorContent">
      <?gmfgen generated="true"?>
      <navigatorContent 
            id="org.supremica.external.sag.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.supremica.external.sag.diagram.navigator.SagNavigatorContentProvider" 
            labelProvider="org.supremica.external.sag.diagram.navigator.SagNavigatorLabelProvider"
            icon="icons/obj16/SagDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="sag_diagram"/>
               </and>
               <instanceof value="org.supremica.external.sag.diagram.navigator.SagAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.supremica.external.sag.diagram.navigator.SagAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.supremica.external.sag.diagram.navigatorSorter" 
               class="org.supremica.external.sag.diagram.navigator.SagNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="sag_diagram"/>
                  </and>
                  <instanceof value="org.supremica.external.sag.diagram.navigator.SagAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <actionProvider
            id="org.supremica.external.sag.diagram.navigatorActionProvider"
            class="org.supremica.external.sag.diagram.navigator.SagNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="org.supremica.external.sag.diagram.navigator.SagAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
