<?xml version="1.0" encoding="UTF-8"?>
<FBType xmlns="http://www.holobloc.com/xml/LibraryElement" Name="Kulbana_composit" >
  <VersionInfo  Author="Goran Cengic" Organization="Chalmers" Version="1.0" Date="2005-07-13" />
  <InterfaceList>
    <EventInputs>
	  <Event Name="INIT" />
      <Event Name="InPortvakt" />
      <Event Name="UrPortvakt" />
      <Event Name="UppMatlyft" />
      <Event Name="Mat" />
      <Event Name="UrMatning" />
      <Event Name="UppHissVan1" />
      <Event Name="UppHissVan2" />
      <Event Name="UtVan1" />
      <Event Name="UtVan2" />
      <Event Name="UppArmVan1" />
      <Event Name="UppArmVan2" />
      <Event Name="ArmHem" />
    </EventInputs>
    <EventOutputs>
	  <Event Name="INITO" />
      <Event Name="KulaPortvak" />
      <Event Name="KulaMatlyft" />
      <Event Name="MatlyftNere" />
      <Event Name="KulaMatstation" />
      <Event Name="StorKula" />
      <Event Name="LitenKula" />
      <Event Name="KulaHiss" />
      <Event Name="HissNere" />
      <Event Name="KulaVan1" />
      <Event Name="KulaVan2" />
      <Event Name="PlockaVan1" />
      <Event Name="PlockaVan2" />
      <Event Name="ArmVan1" />
      <Event Name="ArmVan2" />
      <Event Name="ArmHemma" />
    </EventOutputs>
  </InterfaceList>
  <FBNetwork>

    <FB Name="cycle" Type="E_CYCLE" />
    <FB Name="initsplit" Type="E_SPLIT2" />
    <FB Name="adlink" Type="ADLINK_IO" />

    <FB Name="split7" Type="E_SPLIT7" />
    <FB Name="merge7" Type="E_MERGE7" />

    <FB Name="pv" Type="PV_model" />
    <FB Name="ml" Type="ML_model" />
    <FB Name="ms" Type="MS_model" />
    <FB Name="hs" Type="HS_model" />
    <FB Name="vn1" Type="VN_model" />
    <FB Name="vn2" Type="VN_model" />
    <FB Name="ar" Type="AR_model" />

	<EventConnections>

	  <Connection Source="INIT" Destination="initsplit.EI" />
	  <Connection Source="initsplit.EO1" Destination="INITO" />
	  <Connection Source="initsplit.EO2" Destination="cycle.START" />

	  <Connection Source="cycle.EO" Destination="adlink.READ" />

	  <!-- Values split -->
	  <Connection Source="adlink.VALUES" Destination="split7.EI" />
	  <Connection Source="split7.EO1" Destination="pv.Values" />
	  <Connection Source="split7.EO2" Destination="ml.Values" />
	  <Connection Source="split7.EO3" Destination="ms.Values" />
	  <Connection Source="split7.EO4" Destination="hs.Values" />
	  <Connection Source="split7.EO5" Destination="vn1.Values" />
	  <Connection Source="split7.EO6" Destination="vn2.Values" />
	  <Connection Source="split7.EO7" Destination="ar.Values" />

	  <!-- Write merge -->
	  <Connection Source="pv.Write" Destination="merge7.EI1" />
	  <Connection Source="ml.Write" Destination="merge7.EI2" />
	  <Connection Source="ms.Write" Destination="merge7.EI3" />
	  <Connection Source="hs.Write" Destination="merge7.EI4" />
	  <Connection Source="vn1.Write" Destination="merge7.EI5" />
	  <Connection Source="vn2.Write" Destination="merge7.EI6" />
	  <Connection Source="ar.Write" Destination="merge7.EI7" />
	  <Connection Source="merge7.EO" Destination="adlink.WRITE" />
	  
	  <!-- Portvakt -->
	  <Connection Source="InPortvakt" Destination="pv.InPortvakt" />
	  <Connection Source="UrPortvakt" Destination="pv.UrPortvakt" />
	  <Connection Source="pv.KulaPortvakt" Destination="KulaPortvakt" />

	  <!-- Matlyft -->
	  <Connection Source="UppMatlyft" Destination="ml.UppMatlyft" />
	  <Connection Source="ml.KulaMatlyft" Destination="KulaMatlyft" />
	  <Connection Source="ml.MatlyftNere" Destination="MatlyftNere" />

	  <!-- Matstation -->
	  <Connection Source="Mat" Destination="ms.Mat" />
	  <Connection Source="UrMatning" Destination="ms.UrMatning" />
	  <Connection Source="ms.KulaMatstation" Destination="KulaMatstation" />
	  <Connection Source="ms.StorKula" Destination="StorKula" />
	  <Connection Source="ms.LitenKula" Destination="LitenKula" />

	  <!-- Hiss -->
	  <Connection Source="UppHissVan1" Destination="hs.UppHissVan1" />
	  <Connection Source="UppHissVan2" Destination="hs.UppHissVan2" />
	  <Connection Source="hs.KulaHiss" Destination="KulaHiss" />
	  <Connection Source="hs.KulaVan1" Destination="KulaVan1" />
	  <Connection Source="hs.KulaVan2" Destination="KulaVan2" />
	  <Connection Source="hs.HissNere" Destination="HissNere" />

	  <!-- Vaning 1 -->
	  <Connection Source="UtVan1" Destination="vn1.UtVan" />
	  <Connection Source="vn1.PlockaVan" Destination="PlockaVan1" />

	  <!-- Vaning 2 -->
	  <Connection Source="UtVan2" Destination="vn2.UtVan" />
	  <Connection Source="vn2.PlockaVan" Destination="PlockaVan2" />

	  <!-- Arm -->
	  <Connection Source="UppArmVan1" Destination="ar.UppArmVan1" />
	  <Connection Source="UppArmVan2" Destination="ar.UppArmVan2" />
	  <Connection Source="ArmHem" Destination="ar.ArmHem" />
	  <Connection Source="ar.ArmVan1" Destination="ArmVan1" />
	  <Connection Source="ar.ArmVan2" Destination="ArmVan2" />
	  <Connection Source="ar.ArmHemma" Destination="ArmHemma" />

	</EventConnections>

	<DataConnections>

	  <Connection Source="200" Destination="cycle.DT" />

	  <!-- Portvakt signals -->	
	  <Connection Source="adlink.INPUT0" Destination="pv.KulaPortvakt_sig" />
 	  <Connection Source="pv.InPortvakt_sig" Destination="adlink.OUTPUT0" />
	  <Connection Source="pv.UrPortvakt_sig" Destination="adlink.OUTPUT1" />	

	  <!-- Matlyft signals -->	
	  <Connection Source="adlink.INPUT1" Destination="ml.MatlyftNere_sig" />
	  <Connection Source="adlink.INPUT2" Destination="ml.KulaMatlyft_sig" />
	  <Connection Source="adlink.INPUT3" Destination="ml.MatlyftUppe_sig" />
	  <Connection Source="adlink.INPUT4" Destination="ml.KulaMatstation_sig" />
 	  <Connection Source="ml.UppMatlyft_sig" Destination="adlink.OUTPUT2" />

	  <!-- Matstation signals -->	
	  <Connection Source="adlink.INPUT4" Destination="ms.KulaMatstation_sig" />
	  <Connection Source="adlink.INPUT5" Destination="ms.StorKula_sig" />
	  <Connection Source="adlink.INPUT6" Destination="ms.LitenKula_sig" />
 	  <Connection Source="ms.Mat_sig" Destination="adlink.OUTPUT4" />
 	  <Connection Source="ms.UrMatning_sig" Destination="adlink.OUTPUT3" />

	  <!-- Hiss signal -->
	  <Connection Source="adlink.INPUT7" Destination="hs.HissNere_sig" />
	  <Connection Source="adlink.INPUT10" Destination="hs.KulaHiss_sig" />
	  <Connection Source="adlink.INPUT11" Destination="hs.HissVan1_sig" />
	  <Connection Source="adlink.INPUT12" Destination="hs.KulaVan1_sig" />
	  <Connection Source="adlink.INPUT14" Destination="hs.HissVan2_sig" />
	  <Connection Source="adlink.INPUT15" Destination="hs.KulaVan2_sig" />
 	  <Connection Source="hs.UppHissVan1_sig" Destination="adlink.OUTPUT5" />
 	  <Connection Source="hs.UppHissVan2_sig" Destination="adlink.OUTPUT6" />
 	  
	  <!-- Vaning 1 signals -->	
	  <Connection Source="adlink.INPUT12" Destination="vn1.KulaVan_sig" />
	  <Connection Source="adlink.INPUT13" Destination="vn1.PlockaVan_sig" />
	  <Connection Source="adlink.INPUT22" Destination="vn1.KulaFast_sig" />
	  <Connection Source="adlink.INPUT20" Destination="vn1.ArmVan_sig" />
	  <Connection Source="adlink.INPUT17" Destination="vn1.ArmHemma_sig" />
 	  <Connection Source="vn1.UtVan_sig" Destination="adlink.OUTPUT7" />
 	  <Connection Source="vn1.LyftVan_sig" Destination="adlink.OUTPUT10" />

	  <!-- Vaning 2 signals -->	
	  <Connection Source="adlink.INPUT15" Destination="vn2.KulaVan_sig" />
	  <Connection Source="adlink.INPUT16" Destination="vn2.PlockaVan_sig" />
	  <Connection Source="adlink.INPUT22" Destination="vn2.KulaFast_sig" />
	  <Connection Source="adlink.INPUT21" Destination="vn2.ArmVan_sig" />
	  <Connection Source="adlink.INPUT17" Destination="vn2.ArmHemma_sig" />
 	  <Connection Source="vn2.UtVan_sig" Destination="adlink.OUTPUT11" />
 	  <Connection Source="vn2.LyftVan_sig" Destination="adlink.OUTPUT12" />

	  <!-- Arm signals -->
	  <Connection Source="adlink.INPUT20" Destination="ar.ArmVan1_sig" />
	  <Connection Source="adlink.INPUT13" Destination="ar.KulaVan1_sig" />
	  <Connection Source="adlink.INPUT21" Destination="ar.ArmVan2_sig" />
	  <Connection Source="adlink.INPUT16" Destination="ar.KulaVan2_sig" />
	  <Connection Source="adlink.INPUT22" Destination="ar.KulaFast_sig" />
	  <Connection Source="adlink.INPUT17" Destination="ar.ArmHemma_sig" />
 	  <Connection Source="ar.UppArmVan1_sig" Destination="adlink.OUTPUT13" />
 	  <Connection Source="ar.UppArmVan2_sig" Destination="adlink.OUTPUT14" />
 	  <Connection Source="ar.VridArmHoger_sig" Destination="adlink.OUTPUT15" />
 	  <Connection Source="ar.Sug_sig" Destination="adlink.OUTPUT16" />


	</DataConnections>

  </FBNetwork>

</FBType>
