<EventRef Name="red"/>
</EventRefList>
</State>
<State Name="red_green" Initial="true">
<EventRefList>
<EventRef Name="green"/>
<EventRef Name="red"/>
</EventRefList>
</State>
<State Name="red_green_blue" Initial="true">
<EventRefList>
<EventRef Name="blue"/>
<EventRef Name="green"/>
<EventRef Name="red"/>
</EventRefList>
</State>
<State Name="red_green_blue_default" Initial="true">
<EventRefList>
<EventRef Name="blue"/>
<EventRef Name="default"/>
<EventRef Name="green"/>
<EventRef Name="red"/>
</EventRefList>
</State>
</StateList>
</Automaton>
</AutomataList>
</ProductDES>

