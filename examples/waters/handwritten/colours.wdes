<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProductDES Name="colours"
   xmlns:ns2="http://waters.sourceforge.net/xsd/base"
   xmlns="http://waters.sourceforge.net/xsd/des">
<ns2:Comment>A simple test to see whether coloured markings are displayed and compiled correctly.</ns2:Comment>
<EventList>
<Event Kind="PROPOSITION" Name="blue"/>
<Event Kind="PROPOSITION" Name="cyan"/>
<Event Kind="PROPOSITION" Name="default"/>
<Event Kind="PROPOSITION" Name="green"/>
<Event Kind="PROPOSITION" Name="red"/>
<Event Kind="PROPOSITION" Name="yellow"/>
</EventList>
<AutomataList>
<Automaton Kind="SPEC" Name="colourful">
<EventRefList>
<EventRef Name="blue"/>
<EventRef Name="cyan"/>
<EventRef Name="default"/>
<EventRef Name="green"/>
<EventRef Name="red"/>
<EventRef Name="yellow"/>
</EventRefList>
<StateList>
<State Name="default" Initial="true">
<EventRefList>
<EventRef Name="default"/>
</EventRefList>
</State>
<State Name="more_than_four" Initial="true">
<EventRefList>
<EventRef Name="blue"/>
<EventRef Name="cyan"/>
<EventRef Name="green"/>
<EventRef Name="red"/>
<EventRef Name="yellow"/>
</EventRefList>
</State>
<State Name="red" Initial="true">
