<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ProductDES Name="markedvar" xmlns:ns2="http://waters.sourceforge.net/xsd/des" xmlns="http://waters.sourceforge.net/xsd/base">
    <Comment>A test involving an EFA variable with marked states.</Comment>
    <ns2:EventList>
        <ns2:Event Kind="PROPOSITION" Name=":accepting"/>
    </ns2:EventList>
    <ns2:AutomataList>
        <ns2:Automaton Kind="PLANT" Name="markedvar">
            <ns2:EventRefList>
                <ns2:EventRef Name=":accepting"/>
            </ns2:EventRefList>
            <ns2:StateList>
                <ns2:State Name="0">
                    <ns2:EventRefList>
                        <ns2:EventRef Name=":accepting"/>
                    </ns2:EventRefList>
                </ns2:State>
                <ns2:State Initial="true" Name="1"/>
            </ns2:StateList>
        </ns2:Automaton>
    </ns2:AutomataList>
</ns2:ProductDES>
