                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="184" Y="80"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Source="s1" Target="s0">
                        <LabelBlock>
                            <SimpleIdentifier Name="toggle"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-23" Y="10"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="184" Y="112"/>
