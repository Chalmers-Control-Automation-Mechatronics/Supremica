<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ProductDES Name="patrik3" xmlns:ns2="http://waters.sourceforge.net/xsd/des" xmlns="http://waters.sourceforge.net/xsd/base">
    <Comment>A test for EFA compilation. The compiled automaton representing v should contain a single transition 0-e-&gt;1.</Comment>
    <ns2:EventList>
        <ns2:Event Kind="CONTROLLABLE" Name="e"/>
    </ns2:EventList>
    <ns2:AutomataList>
        <ns2:Automaton Kind="PLANT" Name="v">
            <ns2:EventRefList>
                <ns2:EventRef Name="e"/>
            </ns2:EventRefList>
            <ns2:StateList>
                <ns2:State Initial="true" Name="0"/>
                <ns2:State Name="1"/>
            </ns2:StateList>
            <ns2:TransitionList>
                <ns2:Transition Event="e" Source="0" Target="1"/>
            </ns2:TransitionList>
        </ns2:Automaton>
    </ns2:AutomataList>
</ns2:ProductDES>
