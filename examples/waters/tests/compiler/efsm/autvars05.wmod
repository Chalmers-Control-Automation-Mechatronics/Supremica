<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="autvars05" xmlns="http://waters.sourceforge.net/xsd/module" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns:ns3="http://waters.sourceforge.net/xsd/des">
    <ns2:Comment>A compiler test with automaton variables.
The guard 'mach1==mach2' in the plant 'cond' ensures that event eq is only possible when mach1 and mach2 are in the same state. This means that they must be either 'working' or 'down' (as 'idle1' and 'idle2' are different names and not considered equal).</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="CONTROLLABLE" Name="start1"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="finish1"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="break1"/>
        <EventDecl Kind="CONTROLLABLE" Name="repair1"/>
        <EventDecl Kind="CONTROLLABLE" Name="start2"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="finish2"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="break2"/>
        <EventDecl Kind="CONTROLLABLE" Name="repair2"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="eq"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="mach1">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="idle1">
                        <PointGeometry>
                            <Point X="96" Y="16"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point X="1" Y="-19"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="18" Y="-10"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="working">
                        <PointGeometry>
                            <Point X="48" Y="96"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-34" Y="9"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="down">
                        <PointGeometry>
                            <Point X="144" Y="96"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-6" Y="11"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Source="idle1" Target="working">
                        <LabelBlock>
                            <SimpleIdentifier Name="start1"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-56" Y="-19"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="57" Y="48"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Source="working" Target="idle1">
                        <LabelBlock>
                            <SimpleIdentifier Name="finish1"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-11" Y="1"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="79" Y="60"/>
                        </SplineGeometry>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                    <Edge Source="working" Target="down">
                        <LabelBlock>
                            <SimpleIdentifier Name="break1"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-20" Y="7"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                    <Edge Source="down" Target="idle1">
                        <LabelBlock>
                            <SimpleIdentifier Name="repair1"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="7" Y="-15"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="PLANT" Name="cond">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point X="112" Y="96"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="0" Y="10"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Source="S0" Target="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="eq"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="5" Y="-18"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="157" Y="98"/>
                        </SplineGeometry>
                        <GuardActionBlock>
                            <Guards>
                                <BinaryExpression Operator="==">
                                    <SimpleIdentifier Name="mach1"/>
                                    <SimpleIdentifier Name="mach2"/>
                                </BinaryExpression>
                            </Guards>
                            <LabelGeometry Anchor="NW">
                                <Point X="7" Y="-3"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="PLANT" Name="mach2">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="idle2">
                        <PointGeometry>
                            <Point X="96" Y="16"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point X="1" Y="-19"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="14" Y="-9"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="working">
                        <PointGeometry>
                            <Point X="48" Y="96"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-34" Y="9"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="down">
                        <PointGeometry>
                            <Point X="144" Y="96"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-6" Y="11"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Source="idle2" Target="working">
                        <LabelBlock>
                            <SimpleIdentifier Name="start2"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-56" Y="-19"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="57" Y="48"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Source="working" Target="idle2">
                        <LabelBlock>
                            <SimpleIdentifier Name="finish2"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-11" Y="1"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="79" Y="60"/>
                        </SplineGeometry>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                    <Edge Source="working" Target="down">
                        <LabelBlock>
                            <SimpleIdentifier Name="break2"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-20" Y="7"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                    <Edge Source="down" Target="idle2">
                        <LabelBlock>
                            <SimpleIdentifier Name="repair2"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="7" Y="-15"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <GuardActionBlock>
                            <LabelGeometry Anchor="NW">
                                <Point X="0" Y="20"/>
                            </LabelGeometry>
                        </GuardActionBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
