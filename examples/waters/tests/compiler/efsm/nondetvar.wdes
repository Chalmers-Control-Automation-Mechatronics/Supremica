<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ProductDES Name="nondetvar" xmlns:ns2="http://waters.sourceforge.net/xsd/des" xmlns="http://waters.sourceforge.net/xsd/base">
    <Comment>A test that involves an EFA variable that is explicitly specified to change nondeterministically on a transition.</Comment>
    <ns2:EventList>
        <ns2:Event Kind="CONTROLLABLE" Name="e"/>
    </ns2:EventList>
    <ns2:AutomataList>
        <ns2:Automaton Kind="SPEC" Name="spec">
            <ns2:EventRefList>
                <ns2:EventRef Name="e"/>
            </ns2:EventRefList>
            <ns2:StateList>
                <ns2:State Initial="true" Name="S0"/>
            </ns2:StateList>
            <ns2:TransitionList>
                <ns2:Transition Event="e" Source="S0" Target="S0"/>
            </ns2:TransitionList>
        </ns2:Automaton>
        <ns2:Automaton Kind="PLANT" Name="x">
            <ns2:EventRefList>
                <ns2:EventRef Name="e"/>
            </ns2:EventRefList>
            <ns2:StateList>
                <ns2:State Initial="true" Name="0"/>
                <ns2:State Name="1"/>
                <ns2:State Name="2"/>
            </ns2:StateList>
            <ns2:TransitionList>
                <ns2:Transition Event="e" Source="0" Target="0"/>
                <ns2:Transition Event="e" Source="0" Target="1"/>
                <ns2:Transition Event="e" Source="0" Target="2"/>
                <ns2:Transition Event="e" Source="1" Target="0"/>
                <ns2:Transition Event="e" Source="1" Target="1"/>
                <ns2:Transition Event="e" Source="1" Target="2"/>
                <ns2:Transition Event="e" Source="2" Target="0"/>
                <ns2:Transition Event="e" Source="2" Target="1"/>
                <ns2:Transition Event="e" Source="2" Target="2"/>
            </ns2:TransitionList>
        </ns2:Automaton>
    </ns2:AutomataList>
</ns2:ProductDES>
