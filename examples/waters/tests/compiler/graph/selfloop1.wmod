<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="selfloop1" xmlns="http://waters.sourceforge.net/xsd/module" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns:ns3="http://waters.sourceforge.net/xsd/des">
    <ns2:Comment>A compiler test. An event is selflooped on all states of the only automaton where it appears. This event can be removed by optimisation from the automaton (but not from the model). As a result, the automaton becomes a one-state event-less automaton, which can also be removed by optimisation.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="CONTROLLABLE" Name="e"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="plant">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point X="96" Y="80"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-29" Y="-6"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Source="S0" Target="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="e"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-20" Y="-10"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="146" Y="81"/>
                        </SplineGeometry>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
