<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="empty_2" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>A test for all abstraction rules to see whether they can handle an automaton with just one state.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="PROPOSITION" Name=":alpha"/>
        <EventDecl Kind="PROPOSITION" Name=":accepting">
            <ColorGeometry>
                <Color Red="0" Green="0" Blue="0"/>
            </ColorGeometry>
        </EventDecl>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="before">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="64" X="64"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="10" X="0"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
