<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="basic_2" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>A basic test for all abstraction rules to see whether they can handle an automaton with than one transition between the same two states.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="PROPOSITION" Name=":accepting">
            <ColorGeometry>
                <Color Red="0" Green="0" Blue="0"/>
            </ColorGeometry>
        </EventDecl>
        <EventDecl Kind="CONTROLLABLE" Name="a"/>
        <EventDecl Kind="CONTROLLABLE" Name="b"/>
        <EventDecl Kind="CONTROLLABLE" Name="c"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="before">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="64" X="64"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-8" X="9"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S1">
                        <PointGeometry>
                            <Point Y="144" X="64"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-6" X="11"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S1" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="a"/>
                            <SimpleIdentifier Name="b"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-18" X="6"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point Y="107" X="75"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Target="S0" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="a"/>
                            <SimpleIdentifier Name="b"/>
                            <SimpleIdentifier Name="c"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-20" X="-19"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point Y="106" X="53"/>
                        </SplineGeometry>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
