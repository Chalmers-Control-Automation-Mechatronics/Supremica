                            <SimpleIdentifier Name="lock"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-16" Y="-20"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point X="176" Y="243"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Source="q5" Target="q1">
                        <LabelBlock>
                            <SimpleIdentifier Name="output1"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="-57" Y="-9"/>
                            </LabelGeometry>
                        </LabelBlock>
