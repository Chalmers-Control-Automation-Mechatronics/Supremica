<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="selfloop_subsumption_02" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
