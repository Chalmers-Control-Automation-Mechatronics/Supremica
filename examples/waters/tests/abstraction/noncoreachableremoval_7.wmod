<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="noncoreachableremoval_7" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>Tests whether selfloops are correctly handled. Also checks that an omega marking is not removed which does not become reachable from an alpha state, until a loop which is outgoing from the omega state is reached.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="PROPOSITION" Name=":alpha">
            <ColorGeometry>
                <Color Red="128" Green="128" Blue="128"/>
            </ColorGeometry>
        </EventDecl>
        <EventDecl Kind="PROPOSITION" Name=":accepting">
            <ColorGeometry>
                <Color Red="0" Green="0" Blue="0"/>
            </ColorGeometry>
        </EventDecl>
        <EventDecl Kind="CONTROLLABLE" Name="e1"/>
        <EventDecl Kind="CONTROLLABLE" Name="e2"/>
        <EventDecl Observable="false" Kind="UNCONTROLLABLE" Name="tau"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="SPEC" Name="before">
            <Graph Deterministic="false">
                <NodeList>
                    <SimpleNode Name="S2">
                        <PointGeometry>
                            <Point Y="112" X="64"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-5" X="-24"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S1">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="64" X="128"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="1" X="-18"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S3">
                        <PointGeometry>
                            <Point Y="64" X="208"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-5" X="11"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="64" X="64"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point Y="0" X="-27"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="0" X="-20"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S4">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                            <SimpleIdentifier Name=":alpha"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="112" X="208"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="10" X="0"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S5">
                        <PointGeometry>
                            <Point Y="112" X="128"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="10" X="0"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S2" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-12" X="3"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S1" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-19" X="-14"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S3" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-13" X="3"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S4" Source="S3">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S5" Source="S4">
                        <LabelBlock>
                            <SimpleIdentifier Name="e2"/>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S1" Source="S5">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-8" X="-30"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S4" Source="S4">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S1" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                        </LabelBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="SPEC" Name="after">
            <Graph Deterministic="false">
                <NodeList>
                    <SimpleNode Name="S1">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="64" X="128"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="1" X="-18"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S3">
                        <PointGeometry>
                            <Point Y="64" X="208"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-5" X="11"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="64" X="64"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point Y="0" X="-27"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="0" X="-20"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S4">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                            <SimpleIdentifier Name=":alpha"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="112" X="208"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="10" X="0"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S5">
                        <PointGeometry>
                            <Point Y="112" X="128"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="10" X="0"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S1" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-19" X="-14"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S3" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-13" X="3"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S4" Source="S3">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S5" Source="S4">
                        <LabelBlock>
                            <SimpleIdentifier Name="e2"/>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S1" Source="S5">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-8" X="-30"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                    <Edge Target="S1" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                        </LabelBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
