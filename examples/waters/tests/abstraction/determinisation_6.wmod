<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="determinisation_6" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>This test checks whether redundant tau transitions originating from loops of silent transitions can be handled correctly by the determinsation of non alpha states algorithm. The critical question here is whether the tau transition from S0 to S2 is retained in the abstraction.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="PROPOSITION" Name=":accepting">
            <ColorGeometry>
                <Color Red="0" Green="0" Blue="0"/>
            </ColorGeometry>
        </EventDecl>
        <EventDecl Kind="CONTROLLABLE" Name="e1"/>
        <EventDecl Observable="false" Kind="UNCONTROLLABLE" Name="tau"/>
        <EventDecl Kind="PROPOSITION" Name=":alpha"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="SPEC" Name="before">
            <Graph Deterministic="false">
                <LabelBlock>
                    <SimpleIdentifier Name=":alpha"/>
                    <LabelGeometry Anchor="NW">
                        <Point Y="190" X="11"/>
                    </LabelGeometry>
                </LabelBlock>
                <NodeList>
                    <SimpleNode Name="S2">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="128" X="144"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="5" X="7"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S1">
                        <PointGeometry>
                            <Point Y="32" X="48"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-15" X="-23"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="128" X="48"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point Y="0" X="-27"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="8" X="-16"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S0" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="0" X="-21"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <StartPointGeometry>
                            <Point Y="32" X="48"/>
                        </StartPointGeometry>
                        <SplineGeometry>
                            <Point Y="80" X="35"/>
                        </SplineGeometry>
                        <EndPointGeometry>
                            <Point Y="128" X="48"/>
                        </EndPointGeometry>
                    </Edge>
                    <Edge Target="S1" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-18" X="3"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point Y="80" X="61"/>
                        </SplineGeometry>
                    </Edge>
                    <Edge Target="S2" Source="S1">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-23" X="-4"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <SplineGeometry>
                            <Point Y="63" X="118"/>
                        </SplineGeometry>
                        <EndPointGeometry>
                            <Point Y="128" X="144"/>
                        </EndPointGeometry>
                    </Edge>
                    <Edge Target="S2" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="2" X="-10"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <StartPointGeometry>
                            <Point Y="128" X="48"/>
                        </StartPointGeometry>
                        <EndPointGeometry>
                            <Point Y="128" X="144"/>
                        </EndPointGeometry>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="SPEC" Name="after">
            <Graph Deterministic="false">
                <LabelBlock>
                    <SimpleIdentifier Name=":alpha"/>
                    <LabelGeometry Anchor="NW">
                        <Point Y="160" X="72"/>
                    </LabelGeometry>
                </LabelBlock>
                <NodeList>
                    <SimpleNode Name="S2">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="48" X="144"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="5" X="7"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="48" X="48"/>
                        </PointGeometry>
                        <InitialArrowGeometry>
                            <Point Y="0" X="-27"/>
                        </InitialArrowGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="8" X="-16"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S2" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="e1"/>
                            <SimpleIdentifier Name="tau"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="2" X="-10"/>
                            </LabelGeometry>
                        </LabelBlock>
                        <StartPointGeometry>
                            <Point Y="48" X="48"/>
                        </StartPointGeometry>
                        <EndPointGeometry>
                            <Point Y="48" X="144"/>
                        </EndPointGeometry>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
