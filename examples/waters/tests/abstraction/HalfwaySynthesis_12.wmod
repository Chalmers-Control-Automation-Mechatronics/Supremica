<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="HalfwaySynthesis_12" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>A simple test for halfway synthesis. This tests whether a dumpstates with outgoing transitions attached to them are cleaned up correctly.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="PROPOSITION" Name=":accepting"/>
        <EventDecl Kind="CONTROLLABLE" Name="c"/>
        <EventDecl Kind="UNCONTROLLABLE" Name="u"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="before">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="init">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point X="112" Y="32"/>
                        </PointGeometry>
                        <LabelGeometry>
                            <Point X="10" Y="-8"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="dump1">
                        <PointGeometry>
                            <Point X="112" Y="112"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point X="-52" Y="-9"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Source="init" Target="dump1">
                        <LabelBlock>
                            <SimpleIdentifier Name="c"/>
                            <LabelGeometry Anchor="NW">
                                <Point X="9" Y="-10"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="PLANT" Name="after">
            <Graph>
                <LabelBlock>
                    <SimpleIdentifier Name="c"/>
                    <LabelGeometry Anchor="NW">
                        <Point X="23" Y="26"/>
                    </LabelGeometry>
                </LabelBlock>
                <NodeList>
                    <SimpleNode Initial="true" Name="init">
                        <PointGeometry>
                            <Point X="112" Y="32"/>
                        </PointGeometry>
                        <LabelGeometry>
                            <Point X="10" Y="-8"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
