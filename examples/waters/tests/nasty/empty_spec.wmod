<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module Name="empty_spec" xmlns:ns2="http://waters.sourceforge.net/xsd/base" xmlns="http://waters.sourceforge.net/xsd/module">
    <ns2:Comment>A test that involves an automaton without any states.
Since automaton empty has no initial state, this model should have no reachable states, so it should be both controllable and nonblocking. Yet, it will be considered uncontrollable and blocking, if the automaton empty is ignored.</ns2:Comment>
    <EventDeclList>
        <EventDecl Kind="UNCONTROLLABLE" Name="u"/>
        <EventDecl Kind="PROPOSITION" Name=":accepting"/>
        <EventDecl Kind="CONTROLLABLE" Name="c"/>
    </EventDeclList>
    <ComponentList>
        <SimpleComponent Kind="PLANT" Name="plant">
            <Graph>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <EventList>
                            <SimpleIdentifier Name=":accepting"/>
                        </EventList>
                        <PointGeometry>
                            <Point Y="48" X="80"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-9" X="10"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S1">
                        <PointGeometry>
                            <Point Y="112" X="80"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="-8" X="9"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
                <EdgeList>
                    <Edge Target="S1" Source="S0">
                        <LabelBlock>
                            <SimpleIdentifier Name="u"/>
                            <LabelGeometry Anchor="NW">
                                <Point Y="-8" X="10"/>
                            </LabelGeometry>
                        </LabelBlock>
                    </Edge>
                </EdgeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="SPEC" Name="spec">
            <Graph>
                <LabelBlock>
                    <SimpleIdentifier Name="u"/>
                    <LabelGeometry Anchor="NW">
                        <Point Y="37" X="113"/>
                    </LabelGeometry>
                </LabelBlock>
                <NodeList>
                    <SimpleNode Initial="true" Name="S0">
                        <PointGeometry>
                            <Point Y="48" X="80"/>
                        </PointGeometry>
                        <LabelGeometry Anchor="NW">
                            <Point Y="8" X="3"/>
                        </LabelGeometry>
                    </SimpleNode>
                </NodeList>
            </Graph>
        </SimpleComponent>
        <SimpleComponent Kind="SPEC" Name="empty">
            <Graph>
                <LabelBlock>
                    <SimpleIdentifier Name="c"/>
                    <LabelGeometry Anchor="NW">
                        <Point Y="29" X="26"/>
                    </LabelGeometry>
                </LabelBlock>
            </Graph>
        </SimpleComponent>
    </ComponentList>
</Module>
