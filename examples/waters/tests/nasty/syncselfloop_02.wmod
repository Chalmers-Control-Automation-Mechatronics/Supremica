                        <LabelGeometry Anchor="NW">
                            <Point X="-25" Y="-6"/>
                        </LabelGeometry>
                    </SimpleNode>
                    <SimpleNode Name="S2">
                        <PointGeometry>
                            <Point X="112" Y="224"/>
