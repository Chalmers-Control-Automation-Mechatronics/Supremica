<?xml version="1.0" encoding="utf-8"?>
<Automata name="Parallel Manufacturing Example" comment="This example was found in Ryan James Leducs PhD-thesis 'Hierarchical Interface-based Supervisory Control'. The system is controllable and non-blocking.">
<!-- PLANTS FOR THE HIGH LEVEL -->
<Automaton name="HighLevel-PackagingSystem" type="Plant">
	<Events>
		<Event id="take_item" controllable="true"/>
		<Event id="package" controllable="false"/>
		<Event id="allow_exit" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="take_item"/>
		<Transition source="A1" dest="A2" event="package"/>
		<Transition source="A2" dest="A0" event="allow_exit"/>
	</Transitions>
</Automaton>
<Automaton name="HighLevel-Source" type="Plant">
	<Events>
		<Event id="new_part" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A0" event="new_part"/>
	</Transitions>
</Automaton>
<Automaton name="HighLevel-Sink" type="Plant">
	<Events>
		<Event id="allow_exit" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A0" event="allow_exit"/>
	</Transitions>
</Automaton>
<Automaton name="HighLevel-TestUnit" type="Plant">
	<Events>
		<Event id="part_f_obuff" controllable="true"/>
		<Event id="part_fails" controllable="false"/>
		<Event id="ret_inbuff" controllable="true"/>
		<Event id="part_passes" controllable="true"/>
		<Event id="deposit_part" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
		<State id="A3"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="part_f_obuff"/>
		<Transition source="A1" dest="A2" event="part_fails"/>
		<Transition source="A2" dest="A0" event="ret_inbuff"/>
		<Transition source="A1" dest="A3" event="part_passes"/>
		<Transition source="A3" dest="A0" event="deposit_part"/>
	</Transitions>
</Automaton>

<!-- SUPERVISORS FOR THE HIGH LEVEL -->
<Automaton name="HighLevel-InputBuffer" type="Supervisor">
	<Events>
		<Event id="ret_inbuff" controllable="true"/>
		<Event id="new_part" controllable="true"/>
		<Event id="part_ent-1" controllable="true"/>
		<Event id="part_ent-2" controllable="true"/>
		<Event id="part_ent-3" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
		<State id="A3"/>
		<State id="A4"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="ret_inbuff"/>
		<Transition source="A0" dest="A1" event="new_part"/>
		<Transition source="A1" dest="A0" event="part_ent-1"/>
		<Transition source="A1" dest="A0" event="part_ent-2"/>
		<Transition source="A1" dest="A0" event="part_ent-3"/>
		<Transition source="A1" dest="A2" event="ret_inbuff"/>
		<Transition source="A1" dest="A2" event="new_part"/>
		<Transition source="A2" dest="A1" event="part_ent-1"/>
		<Transition source="A2" dest="A1" event="part_ent-2"/>
		<Transition source="A2" dest="A1" event="part_ent-3"/>
		<Transition source="A2" dest="A3" event="ret_inbuff"/>
		<Transition source="A2" dest="A3" event="new_part"/>
		<Transition source="A3" dest="A2" event="part_ent-1"/>
		<Transition source="A3" dest="A2" event="part_ent-2"/>
		<Transition source="A3" dest="A2" event="part_ent-3"/>
		<Transition source="A3" dest="A4" event="ret_inbuff"/>
		<Transition source="A3" dest="A4" event="new_part"/>
		<Transition source="A4" dest="A3" event="part_ent-1"/>
		<Transition source="A4" dest="A3" event="part_ent-2"/>
		<Transition source="A4" dest="A3" event="part_ent-3"/>
	</Transitions>
</Automaton>
<Automaton name="HighLevel-OutputBuffer" type="Supervisor">
	<Events>
		<Event id="part_ent-1" controllable="true"/>
		<Event id="part_ent-2" controllable="true"/>
		<Event id="part_ent-3" controllable="true"/>
		<Event id="fin_exit-1" controllable="false"/>
		<Event id="fin_exit-2" controllable="false"/>
		<Event id="fin_exit-3" controllable="false"/>
		<Event id="part_f_obuff" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
		<State id="A3"/>
		<State id="A4"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A0" event="part_ent-1"/>
		<Transition source="A0" dest="A0" event="part_ent-2"/>
		<Transition source="A0" dest="A0" event="part_ent-3"/>
		<Transition source="A0" dest="A1" event="fin_exit-1"/>
		<Transition source="A0" dest="A1" event="fin_exit-2"/>
		<Transition source="A0" dest="A1" event="fin_exit-3"/>
		<Transition source="A1" dest="A0" event="part_f_obuff"/>
		<Transition source="A1" dest="A1" event="part_ent-1"/>
		<Transition source="A1" dest="A1" event="part_ent-2"/>
		<Transition source="A1" dest="A1" event="part_ent-3"/>
		<Transition source="A1" dest="A2" event="fin_exit-1"/>
		<Transition source="A1" dest="A2" event="fin_exit-2"/>
		<Transition source="A1" dest="A2" event="fin_exit-3"/>
		<Transition source="A2" dest="A1" event="part_f_obuff"/>
		<Transition source="A2" dest="A3" event="fin_exit-1"/>
		<Transition source="A2" dest="A3" event="fin_exit-2"/>
		<Transition source="A2" dest="A3" event="fin_exit-3"/>
		<Transition source="A3" dest="A2" event="part_f_obuff"/>
		<Transition source="A3" dest="A4" event="fin_exit-1"/>
		<Transition source="A3" dest="A4" event="fin_exit-2"/>
		<Transition source="A3" dest="A4" event="fin_exit-3"/>
		<Transition source="A4" dest="A3" event="part_f_obuff"/>
	</Transitions>
</Automaton>
<Automaton name="HighLevel-PackageBuffer" type="Supervisor">
	<Events>
		<Event id="deposit_part" controllable="true"/>
		<Event id="take_item" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
		<State id="A3"/>
		<State id="A4"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="deposit_part"/>
		<Transition source="A1" dest="A0" event="take_item"/>
		<Transition source="A1" dest="A2" event="deposit_part"/>
		<Transition source="A2" dest="A1" event="take_item"/>
		<Transition source="A2" dest="A3" event="deposit_part"/>
		<Transition source="A3" dest="A2" event="take_item"/>
		<Transition source="A3" dest="A4" event="deposit_part"/>
		<Transition source="A4" dest="A3" event="take_item"/>
	</Transitions>
</Automaton>

<!-- EXTRA SUPERVISOR TO ENSURE NON-BLOCKING (see Section 9.3 in Leducs PhD thesis) -->
<Automaton name="HighLevel-MaterialFeedbackSupervisor" type="Supervisor">
	<Events>
		<Event id="new_part" controllable="true"/>
		<Event id="part_passes" controllable="true"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
		<State id="A2"/>
		<State id="A3"/>
		<State id="A4"/>
<!--	<State id="A5"/>
		<State id="A6"/> -->
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="new_part"/>
		<Transition source="A1" dest="A0" event="part_passes"/>
		<Transition source="A1" dest="A2" event="new_part"/>
		<Transition source="A2" dest="A1" event="part_passes"/>
		<Transition source="A2" dest="A3" event="new_part"/>
		<Transition source="A3" dest="A2" event="part_passes"/>
		<Transition source="A3" dest="A4" event="new_part"/>
		<Transition source="A4" dest="A3" event="part_passes"/>
<!--	<Transition source="A4" dest="A5" event="new_part"/>
		<Transition source="A5" dest="A4" event="part_passes"/>
		<Transition source="A5" dest="A6" event="new_part"/>
		<Transition source="A6" dest="A5" event="part_passes"/> -->
	</Transitions>
</Automaton>

<!-- INTERFACE BETWEEN THE HIGH LEVEL AND LOW LEVEL 1 -->
<Automaton name="Interface HighLevel/LowLevel1" type="Interface">
	<Events>
		<Event id="part_ent-1" controllable="true"/>
		<Event id="fin_exit-1" controllable="false"/>
	</Events>
	<States>
		<State id="A0" initial="true" accepting="true"/>
		<State id="A1"/>
	</States>
	<Transitions>
		<Transition source="A0" dest="A1" event="part_ent-1"/>
		<Transition source="A1" dest="A0" event="fin_exit-1"/>
	</Transitions>
</Automaton>

