<?xml version="1.0" encoding="ISO-8859-1"?>
<Automata name="" major="0" minor="9" comment="This testcase is based on the Transfer Line example from 'Notes on Control of Discrete-Event Systems' my W.M Wonham. Here with 1 serially connected, identical cells.">
<Automaton name="B1" type="Specification">
	<Events>
		<Event id="0" label="M1_1_finished" controllable="false"/>
		<Event id="1" label="M2_1_started"/>
		<Event id="2" label="TU_1_reject" controllable="false"/>
	</Events>
	<States>
