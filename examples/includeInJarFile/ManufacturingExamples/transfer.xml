	<Events>
		<Event id="0" label="M1_1_finished" controllable="false"/>
		<Event id="1" label="start"/>
	</Events>
	<States>
		<State id="0" name="I1" initial="true" accepting="true"/>
		<State id="1" name="W1"/>
	</States>
	<Transitions>
		<Transition source="0" dest="1" event="1"/>
		<Transition source="1" dest="0" event="0"/>
	</Transitions>
</Automaton>
