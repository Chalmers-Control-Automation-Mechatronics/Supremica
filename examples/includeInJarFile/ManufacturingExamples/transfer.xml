		<State id="2" name="2"/>
	</States>
	<Transitions>
		<Transition source="0" dest="1" event="0"/>
		<Transition source="0" dest="1" event="2"/>
		<Transition source="1" dest="0" event="1"/>
		<Transition source="1" dest="2" event="0"/>
		<Transition source="1" dest="2" event="2"/>
