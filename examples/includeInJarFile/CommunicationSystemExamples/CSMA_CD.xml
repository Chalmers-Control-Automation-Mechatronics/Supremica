<?xml version="1.0" encoding="utf-8"?><Automata name="CSMA/CD protocol" comment="The Carrier Sense, Multiple Access with Collision Detection (CSMA/CD) protocol. The two MACs (Media Access Control) are sending messages to each other over a common medium. If two messages collide in the medium, they are both lost and have to be resent. The correct protocol behaviour (modelled by the specifications) should be observation equivalent to the suggested implementation (the plants) with send1, send2, rec1 and rec2 as the only observable events. This can be verified for example by, in turn, minimizing the plants together and the specifications together (compositionally), keeping only the mentioned events. The minimized plant and specification models can then be verified (by visual inspection as of now) to be observation equivalent. Adapted from 'Verifying a CSMA/CD-protocol with CCS' by Joachim Parrow."><Automaton name="B12" type="Specification">	<Events>		<Event id="s" label="send1"/>		<Event id="p" label="p1"/>		<Event id="r" label="rec2"/>		<Event id="v" label="v1"/>	</Events>	<States>		<State id="b0" initial="true"/>		<State id="b1"/>		<State id="b2"/>		<State id="b3"/>		<State id="b4"/>		<State id="b5"/>	</States>	<Transitions>		<Transition source="b0" dest="b1" event="s"/>		<Transition source="b1" dest="b2" event="p"/>		<Transition source="b2" dest="b3" event="s"/>		<Transition source="b3" dest="b4" event="r"/>		<Transition source="b4" dest="b1" event="v"/>		<Transition source="b2" dest="b5" event="r"/>		<Transition source="b5" dest="b0" event="v"/>	</Transitions></Automaton><Automaton name="B21" type="Specification">	<Events>		<Event id="s" label="send2"/>		<Event id="p" label="p2"/>		<Event id="r" label="rec1"/>		<Event id="v" label="v2"/>	</Events>	<States>		<State id="b0" initial="true"/>		<State id="b1"/>		<State id="b2"/>		<State id="b3"/>		<State id="b4"/>		<State id="b5"/>	</States>	<Transitions>		<Transition source="b0" dest="b1" event="s"/>		<Transition source="b1" dest="b2" event="p"/>		<Transition source="b2" dest="b3" event="s"/>		<Transition source="b3" dest="b4" event="r"/>		<Transition source="b4" dest="b1" event="v"/>		<Transition source="b2" dest="b5" event="r"/>		<Transition source="b5" dest="b0" event="v"/>	</Transitions></Automaton><Automaton name="Semaphore" type="Specification">	<Events>		<Event id="p1" label="p1"/>		<Event id="p2" label="p2"/>		<Event id="v1" label="v1"/>		<Event id="v2" label="v2"/>	</Events>	<States>		<State id="s0" initial="true"/>		<State id="s1"/>	</States>	<Transitions>		<Transition source="s0" dest="s1" event="p1"/>		<Transition source="s1" dest="s0" event="v1"/>		<Transition source="s0" dest="s1" event="p2"/>		<Transition source="s1" dest="s0" event="v2"/>	</Transitions></Automaton><Automaton name="MAC1" type="Plant">	<Events>		<Event id="s" label="send1"/>		<Event id="b" label="b1"/>		<Event id="c" label="c1"/>		<Event id="e" label="e1"/>		<Event id="br" label="br1"/>		<Event id="er" label="er1"/>		<Event id="r" label="rec1"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m1"/>		<State id="m2"/>		<State id="m3"/>		<State id="m4"/>		<State id="m5"/>		<State id="m6"/>	</States>	<Transitions>		<Transition source="m0" dest="m1" event="s"/>		<Transition source="m1" dest="m2" event="b"/>		<Transition source="m2" dest="m1" event="c"/>		<Transition source="m2" dest="m0" event="e"/>		<Transition source="m0" dest="m3" event="br"/>		<Transition source="m1" dest="m4" event="br"/>		<Transition source="m3" dest="m4" event="s"/>		<Transition source="m3" dest="m5" event="er"/>		<Transition source="m4" dest="m6" event="er"/>		<Transition source="m5" dest="m6" event="s"/>		<Transition source="m5" dest="m0" event="r"/>		<Transition source="m6" dest="m1" event="r"/>	</Transitions></Automaton><Automaton name="MAC2" type="Plant">	<Events>		<Event id="s" label="send2"/>		<Event id="b" label="b2"/>		<Event id="c" label="c2"/>		<Event id="e" label="e2"/>		<Event id="br" label="br2"/>		<Event id="er" label="er2"/>		<Event id="r" label="rec2"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m1"/>		<State id="m2"/>		<State id="m3"/>		<State id="m4"/>		<State id="m5"/>		<State id="m6"/>	</States>	<Transitions>		<Transition source="m0" dest="m1" event="s"/>		<Transition source="m1" dest="m2" event="b"/>		<Transition source="m2" dest="m1" event="c"/>		<Transition source="m2" dest="m0" event="e"/>		<Transition source="m0" dest="m3" event="br"/>		<Transition source="m1" dest="m4" event="br"/>		<Transition source="m3" dest="m4" event="s"/>		<Transition source="m3" dest="m5" event="er"/>		<Transition source="m4" dest="m6" event="er"/>		<Transition source="m5" dest="m6" event="s"/>		<Transition source="m5" dest="m0" event="r"/>		<Transition source="m6" dest="m1" event="r"/>	</Transitions></Automaton><Automaton name="Medium" type="Plant">	<Events>		<Event id="b1" label="b1"/>		<Event id="b2" label="b2"/>		<Event id="c1" label="c1"/>		<Event id="e1" label="e1"/>		<Event id="br1" label="br1"/>		<Event id="er1" label="er1"/>		<Event id="c2" label="c2"/>		<Event id="e2" label="e2"/>		<Event id="br2" label="br2"/>		<Event id="er2" label="er2"/>	</Events>	<States>		<State id="med0" initial="true"/>		<State id="med1"/>		<State id="med2"/>		<State id="med3"/>		<State id="med4"/>		<State id="med5"/>		<State id="med6"/>		<State id="med7"/>		<State id="med8"/>		<State id="med9"/>	</States>	<Transitions>		<Transition source="med0" dest="med1" event="b1"/>		<Transition source="med1" dest="med2" event="br2"/>		<Transition source="med2" dest="med3" event="e1"/>		<Transition source="med0" dest="med4" event="b2"/>		<Transition source="med4" dest="med5" event="br1"/>		<Transition source="med5" dest="med6" event="e2"/>		<Transition source="med3" dest="med0" event="er2"/>		<Transition source="med6" dest="med0" event="er1"/>		<Transition source="med1" dest="med7" event="b2"/>		<Transition source="med2" dest="med7" event="b2"/>		<Transition source="med3" dest="med7" event="b2"/>		<Transition source="med4" dest="med7" event="b1"/>		<Transition source="med5" dest="med7" event="b1"/>		<Transition source="med6" dest="med7" event="b1"/>		<Transition source="med7" dest="med8" event="c1"/>		<Transition source="med7" dest="med9" event="c2"/>		<Transition source="med8" dest="med0" event="c2"/>		<Transition source="med9" dest="med0" event="c1"/>			</Transitions></Automaton><Automaton name="MAC - S" type="Plant">	<Events>		<Event id="s" label="send?"/>		<Event id="b" label="b!"/>		<Event id="c" label="c?"/>		<Event id="e" label="e!"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m1"/>		<State id="m2"/>	</States>	<Transitions>		<Transition source="m0" dest="m1" event="s"/>		<Transition source="m1" dest="m2" event="b"/>		<Transition source="m2" dest="m1" event="c"/>		<Transition source="m2" dest="m0" event="e"/>	</Transitions></Automaton><Automaton name="MAC - R" type="Plant">	<Events>		<Event id="br" label="br?"/>		<Event id="er" label="er?"/>		<Event id="r" label="rec!"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m3"/>		<State id="m5"/>	</States>	<Transitions>		<Transition source="m0" dest="m3" event="br"/>		<Transition source="m3" dest="m5" event="er"/>		<Transition source="m5" dest="m0" event="r"/>	</Transitions></Automaton><Automaton name="MAC - S'" type="Plant">	<Events>		<Event id="s" label="send?"/>		<Event id="b" label="b!"/>		<Event id="c" label="c?"/>		<Event id="e" label="e!"/>		<Event id="book" label="book!"/>		<Event id="unbook" label="unbook?"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m1"/>		<State id="m2"/>		<State id="m3"/>		<State id="m4"/>		<State id="m5"/>	</States>	<Transitions>		<Transition source="m0" dest="m1" event="s"/>		<Transition source="m2" dest="m3" event="b"/>		<Transition source="m3" dest="m4" event="c"/>		<Transition source="m3" dest="m5" event="e"/>		<Transition source="m1" dest="m2" event="book"/>		<Transition source="m2" dest="m1" event="unbook"/>		<Transition source="m4" dest="m1" event="unbook"/>		<Transition source="m5" dest="m0" event="unbook"/>	</Transitions></Automaton><Automaton name="MAC - R'" type="Plant">	<Events>		<Event id="br" label="br?"/>		<Event id="er" label="er?"/>		<Event id="r" label="rec!"/>		<Event id="book" label="book!"/>		<Event id="unbook" label="unbook?"/>	</Events>	<States>		<State id="m0" initial="true"/>		<State id="m1"/>		<State id="m2"/>		<State id="m3"/>		<State id="m4"/>	</States>	<Transitions>		<Transition source="m0" dest="m1" event="book"/>		<Transition source="m1" dest="m0" event="unbook"/>		<Transition source="m1" dest="m2" event="br"/>		<Transition source="m2" dest="m3" event="er"/>		<Transition source="m3" dest="m4" event="r"/>		<Transition source="m4" dest="m0" event="unbook"/></Transitions></Automaton><Automaton name="Sem" type="Plant">	<Events>		<Event id="book" label="book?"/>		<Event id="unbook" label="unbook!"/>	</Events>	<States>		<State id="s0" initial="true"/>		<State id="s1"/>	</States>	<Transitions>		<Transition source="s0" dest="s1" event="book"/>		<Transition source="s1" dest="s0" event="unbook"/></Transitions></Automaton></Automata>